// Models related to the user, api access, and sessions

model User {
    id           String  @id @default(uuid())
    email        String  @unique
    username     String  @unique
    passwordHash String
    ApiKey       ApiKey?
}

model ApiKey {
    id     String @id @default(uuid())
    user   User   @relation(fields: [userId], references: [id])
    userId String @unique
}
